{
  "name": "crawler/crawler-worker-php",
  "description": "Crawler worker on php",
  "license": "MIT",
  "require": {
    "php": "^7.2.5|^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "monolog/monolog": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.8|^9.3.3"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
